<footer class="container-fluid footer">
  <div class="container">
    <div class="row">
      <div class="col-12">
        <% let today = new Date()%>
        
        <% let allMonth = ["Janvier","Fevrié","Mars","Avril","Mais","Juin","Juillet","Aout","Septembre","Octobre","Novembre","Décembre"]%>
        <% let day = today.getDate()%>
        <% let month = allMonth[today.getMonth()]%>
        <% let year = today.getFullYear()%>
        
        <p style="float: right">&copy;Quai des Bananes</p>
        <p style="float: left"><%=  day %> <%= month  %> <%=  year %></p>
      </div>
    </div>
  </div>
</footer>
